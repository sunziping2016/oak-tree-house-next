{
  "name": "oak-tree-house-next",
  "version": "0.1.0",
  "description": "My next generation personal blog powered by Vue 3 and VitePress",
  "scripts": {
    "watch": "nodemon",
    "build": "rimraf -rf dist && tsc -p src/client && tsc -p src/node && node scripts/copyClient.js",
    "lint": "yarn lint:js && yarn lint:ts",
    "lint:js": "prettier --check --write \"{scripts,src}/**/*.js\"",
    "lint:ts": "prettier --check --write --parser typescript \"{scripts,src}/**/*.ts\"",
    "start": "node -r source-map-support/register dist/node/main.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sunziping2016/oak-tree-house-next.git"
  },
  "author": "Sun Ziping",
  "license": "MIT",
  "homepage": "https://github.com/sunziping2016/oak-tree-house-next#readme",
  "bugs": {
    "url": "https://github.com/sunziping2016/oak-tree-house-next/issues"
  },
  "dependencies": {
    "@vitejs/plugin-vue": "^1.0.4",
    "@vue/compiler-sfc": "^3.0.5",
    "chalk": "^4.1.0",
    "fs-extra": "^9.0.1",
    "globby": "^11.0.2",
    "gray-matter": "^4.0.2",
    "markdown-it": "^12.0.4",
    "vite": "^2.0.0-beta.11",
    "vue": "^3.0.5",
    "vue-router": "^4.0.2",
    "vuex": "^4.0.0-rc.2"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.6",
    "@types/markdown-it": "^12.0.1",
    "@types/node": "^14.14.20",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "sass": "^1.32.2",
    "sass-loader": "^10.1.0",
    "source-map-support": "^0.5.19",
    "typescript": "^4.1.3"
  }
}
